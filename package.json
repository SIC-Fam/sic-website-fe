{
  "name": "sic-website-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*": [
      "yarn format",
      "git add ."
    ],
    "./**/*.{ts,js,tsx}": [
      "yarn format",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@material-tailwind/react": "^2.0.4",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "formik": "^2.4.2",
    "history": "^5.3.0",
    "html-react-parser": "^4.0.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.1",
    "react-toastify": "^9.1.3",
    "recoil": "^0.7.7",
    "sass": "^1.63.6",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.6",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/leaflet": "^1.9.3",
    "@types/lodash": "^4.14.195",
    "@types/react-leaflet": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/typescript-estree": "^5.60.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3"
  }
}
